{
  "name": "Entertainment Blocker",
  "description": "Extension prevent you to waste valuable time with Entertainment like streaming services",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_icon": {
      "32": "images/youtube_blocker_default_icon_32.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/main.bundle.js"]
    }
  ],
  "icons": {
    "128": "images/icons/youtube_blocker_128.png"
  }
}
